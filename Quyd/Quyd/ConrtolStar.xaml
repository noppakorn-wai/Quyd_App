<UserControl x:Class="Quyd.ConrtolStar"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="16" d:DesignWidth="100">
    
    <Grid x:Name="LayoutRoot">
    	<Image HorizontalAlignment="Left" Height="16" VerticalAlignment="Top" Width="16" Source="Resources/Images/star_fav_icon&amp;16.png"/>
    	<Image HorizontalAlignment="Left" Height="16" VerticalAlignment="Top" Width="16" Source="Resources/Images/star_fav_icon&amp;16.png" Margin="21,0,0,0"/>
    	<Image HorizontalAlignment="Left" Height="16" VerticalAlignment="Top" Width="16" Source="Resources/Images/star_fav_icon&amp;16.png" Margin="42,0,0,0"/>
    	<Image HorizontalAlignment="Left" Height="16" VerticalAlignment="Top" Width="16" Source="Resources/Images/star_fav_empty_icon&amp;16.png" Margin="63,0,0,0"/>
    	<Image HorizontalAlignment="Left" Height="16" VerticalAlignment="Top" Width="16" Source="Resources/Images/star_fav_empty_icon&amp;16.png" Margin="84,0,0,0"/>

    </Grid>
</UserControl>