<UserControl x:Class="Quyd.Controls.ControlStar"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="24" d:DesignWidth="140">
    
    <Grid x:Name="LayoutRoot">
    	<Image HorizontalAlignment="Left" Height="24" VerticalAlignment="Top" Width="24" Source="/Resources/Images/star_fav_icon&amp;24.png"/>
    	<Image HorizontalAlignment="Left" Height="24" VerticalAlignment="Top" Width="24" Source="/Resources/Images/star_fav_icon&amp;24.png" Margin="29,0,0,0"/>
    	<Image HorizontalAlignment="Left" Height="24" VerticalAlignment="Top" Width="24" Source="/Resources/Images/star_fav_icon&amp;24.png" Margin="58,0,0,0"/>
    	<Image HorizontalAlignment="Left" Height="24" VerticalAlignment="Top" Width="24" Source="/Resources/Images/star_fav_empty_icon&amp;24.png" Margin="87,0,0,0"/>
    	<Image HorizontalAlignment="Left" Height="24" VerticalAlignment="Top" Width="24" Source="/Resources/Images/star_fav_empty_icon&amp;24.png" Margin="116,0,0,0"/>

    </Grid>
</UserControl>