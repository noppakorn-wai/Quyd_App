<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:Quyd="clr-namespace:Quyd.Controls" x:Class="Quyd.Controls.ControlFeed"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignWidth="480" d:DesignHeight="260"
    Width="auto" Height="260" Margin="5,5,5,0">
 
    <Grid x:Name="LayoutRoot">
        <Quyd:ControlUserProfile x:Name="controlUserProfile" VerticalAlignment="Top" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Top" Width="Auto" Height="84"/>
        <Quyd:ControlPost x:Name="controlPost" Margin="0,84,0,0"/>
    </Grid>
    
</UserControl>